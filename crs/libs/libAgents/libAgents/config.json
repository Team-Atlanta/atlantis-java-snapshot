{
    "env": {
      "https_proxy": "",
      "OPENAI_BASE_URL": "",
      "GEMINI_API_KEY": "",
      "OPENAI_API_KEY": "",
      "JINA_API_KEY": "",
      "BRAVE_API_KEY": "",
      "DEFAULT_MODEL_NAME": "",
      "AIXCC_LITELLM_HOSTNAME": "",
      "LITELLM_KEY": ""
    },
    "defaults": {
      "llm_provider": "litellm",
      "step_sleep": 1000
    },
    "providers": {
      "gemini": {
        "createClient": "createGoogleGenerativeAI",
        "clientConfig": {
          "compatibility": "compatible"
        }
      },
      "openai": {
        "createClient": "createOpenAI",
        "clientConfig": {
          "compatibility": "strict"
        }
      },
      "litellm": {
        "createClient": "createOpenAI",
        "clientConfig": {
          "compatibility": "strict"
        }
      }
    },
    "models": {
      "gemini": {
        "default": {
          "model": "gemini-2.5-pro",
          "temperature": 0
        },
        "tools": {
          "dedup": { "temperature": 0.1 },
          "evaluator": {},
          "errorAnalyzer": {},
          "queryRewriter": { "temperature": 0.1 },
          "agent": { "temperature": 0.7 },
          "agentBeastMode": { "temperature": 0.7 }
        }
      },
      "openai": {
        "default": {
          "model": "gpt-4.1",
          "temperature": 0
        },
        "tools": {
          "dedup": { "temperature": 0.1 },
          "evaluator": {},
          "errorAnalyzer": {},
          "queryRewriter": { "temperature": 0.1 },
          "agent": { "temperature": 0.7 },
          "agentBeastMode": { "temperature": 0.7 }
        }
      },
      "litellm": {
        "default": {
          "model": "gpt-4.1",
          "temperature": 0
        },
        "tools": {
          "dedup": { "temperature": 0.1 },
          "evaluator": {},
          "errorAnalyzer": {},
          "queryRewriter": { "temperature": 0.1 },
          "agent": { "temperature": 0.7 },
          "agentBeastMode": { "temperature": 0.7 }
        }
      }
    }
  }
